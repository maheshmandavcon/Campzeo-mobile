      "clerkPublishableKey": "pk_test_ZmFzdC1yZWRmaXNoLTUyLmNsZXJrLmFjY291bnRzLmRldiQ",
      "apiBaseUrl": "https://campzeo-v1-oym2.vercel.app/api/",
      "router": {},
      "eas": {
